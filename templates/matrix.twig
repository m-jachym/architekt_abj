{% for matrixBlock in matrixField.all() %}

    {% switch matrixBlock.type %}

    {% case "modulGraficzny" %}
        {% include "graphic_modules" with {
            graphicBlocks: matrixBlock,
        } %}

    {% case "tytulINaglowek" %}
        {% include "components/title-and-header" with {
            graphicBlocks: matrixBlock,
        } %}

    {% case "backgroundOtwarcie" %}
        <div class="backgroundOpen-{{ matrixBlock.backgroundOtwarcie }}">

    {% case "backgroundZamkniecie" %}
        </div>

    {% case "margines" %}
        <div class="marginOpen-{{ matrixBlock.margines }}"></div>



    {% default %}
        <p>Nie ma obs≈Çugi {{ matrixBlock.type }} (matrix)</p>

    {% endswitch %}
{% endfor %}